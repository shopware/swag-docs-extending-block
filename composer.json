{
    "name": "swag/extending-block",
    "description": "Example plugin which overwrites a storefront twig block",
    "version": "v1.0.0",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "shopware AG"
        }
    ],
    "require": {
        "shopware/core": "*",
        "shopware/storefront": "*"
    },
    "extra": {
        "shopware-plugin-class": "Swag\\ExtendingBlock\\ExtendingBlock",
        "label": {
            "de-DE": "Label für das Plugin ExtendingBlock",
            "en-GB": "Label for the plugin ExtendingBlock"
        },
        "description": {
            "de-DE": "Beschreibung für das Plugin ExtendingBlock",
            "en-GB": "Description for the plugin ExtendingBlock"
        }
    },
    "autoload": {
        "psr-4": {
            "Swag\\ExtendingBlock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swag\\ExtendingBlock\\Tests\\": "tests/"
        }
    }
}
